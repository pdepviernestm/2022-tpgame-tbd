import wollok.game.*
import personajes.*
import eventos.*
program juego {

	//Config
	game.height(7)
	game.width(8)
	game.boardGround("Autopista.png")
	game.cellSize(135)
	game.addVisual(garaje)
	
	//Eventos
	eventos.generarJugador()
	game.onTick(2000,"generarAuto",{=>eventos.generarAutoRojo()})
	game.onTick(2000,"generarArbol",{=> eventos.generarArbol()})
	
	//Start
	game.start()
}